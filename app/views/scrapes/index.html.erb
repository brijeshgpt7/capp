<h1>Listing scrapes</h1>

<table>
  <thead>
    <tr>
      <th>Rank/th>
      <th>School</th>
      <th>Tuition Fees</th>
      <th>Visits count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scrapes.each do |scrape| %>
      <tr>
      	<%= scrape%>
        <td><%= scrape["u.s._news_rank"] %></td>
        <td><%= scrape["school"] %></td>
         <td><%= scrape["tuition_and_fees"] %></td>
      </tr>
    <% end %>
  </tbody>
</table>



